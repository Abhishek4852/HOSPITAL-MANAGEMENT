-- drop table bill;

create table bill(
bid int primary key auto_increment,
patient_name  varchar(100),
pid int references patient(pid),
did_alloted int references patient_allocation(did_alloted),
adimission_fees int not null default 500,
adimission_date date,
consult_charge int default 800,
bed_charge int references bed(charges),
lab_charge int references lab(charges),
surgery_charge int references surgery(charge),
pharmacy_charge int references pharmacy(charges),
no_of_days int not null,
Discharge_date date
);




select * from bill;
select adimission_fees+consult_charge+(bed_charge*no_of_days)+lab_charge+surgery_charge+pharmacy_charge as total_bill from bill where pid = 1;


