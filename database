create table lab(
lid int primary key ,

Test_Name varchar(50) not null,
PID int not null,
mobile_no bigint(11) not null,
symptoms varchar(200),
Test_charge int not null default 500,
final_report varchar(49),
foreign key (PID) references patient (PID)
);


