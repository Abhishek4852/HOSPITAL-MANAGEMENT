create table patient_allocation(
aid int primary key auto_increment,
pid int not null references patient(pid),
did_alloted int references doctor(did),
nid_alloted int references nurse(nid),
sid_alloted int references surgery(sid),
bid_alloted int references bed(bid),
lid_alloted int references lab(lid)
);

insert into patient_allocation (pid, did_alloted, nid_alloted, sid_alloted, bid_alloted, lid_alloted) 
